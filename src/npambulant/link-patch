#!/bin/sh
# Patch to link npambulant on 64 bit
 ../../libtool --tag=CXX   --mode=link g++ -DWITH_GTK -DMOZ_X11 -g -O2 -pthread -module -static -shared ../player_gtk/gtk_mainloop.o -lambulant_gtk -pthread -lgtk-x11-2.0 -lgdk-x11-2.0 -latk-1.0 -lgdk_pixbuf-2.0 -lpangocairo-1.0 -lpango-1.0 -lcairo -lgobject-2.0 -lgmodule-2.0 -ldl -lgthread-2.0 -lrt -lglib-2.0   -lambulant_ffmpeg ../../third_party_packages/ffmpeg/libavcodec/libavcodec.a ../../third_party_packages/ffmpeg/libavformat/libavformat.a ../../third_party_packages/ffmpeg/libavutil/libavutil.a -lz -lbz2 -lm -lfaad -ldl   ../../third_party_packages/ffmpeg/libavcodec/libavcodec.a ../../third_party_packages/ffmpeg/libavutil/libavutil.a -lz -lbz2 -lm -lfaad -ldl   ../../third_party_packages/ffmpeg/libavutil/libavutil.a   -lambulant_sdl `sdl-config --libs`   -L../../src/libambulant/.libs -lambulant -lambulant_live -o npambulant.so -rpath /usr/local/lib np_entry.lo npn_gate.lo npp_gate.lo plugin.lo nsScriptablePeer.lo 
echo copy\ npambulant.so\ to\ the\ installed\ plugins\ directory\ \(~/.mozilla/plugins\ or\ /usr/lib/mozilla/plugins\)