<?xml version="1.0"?>
<!DOCTYPE filter [
	<!ELEMENT filter (systemTest|customTest)* >
	<!ELEMENT systemTest EMPTY>
	<!ATTLIST systemTest name ID #REQUIRED>
	<!ATTLIST systemTest value CDATA #REQUIRED>
	<!ELEMENT customTest EMPTY>
	<!ATTLIST customTest name ID #REQUIRED>
	<!ATTLIST customTest value CDATA #REQUIRED>
]>
<filter>
	<systemTest name="systemAudioDesc" value="on" />
	<systemTest name="systemBitrate" value="10000000" />
	<systemTest name="systemCaptions" value="on" />
	<systemTest name="systemCPU" value="powerpc" />
	<systemTest name="systemLanguage" value="en" />
	<systemTest name="systemOperatingSystem" value="macos" />
	<systemTest name="systemScreenSize" value="1024X1280" />
	<systemTest name="systemScreenDepth" value="32" />
	<systemTest name="systemComponent" value="ambulant:sc1 ambulant:sc2" />	
	<customTest name="testLinking" value="true"/>
</filter>
