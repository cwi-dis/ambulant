AMBULANT_DIR = $(SRCROOT)/../..
AMBULANT_BUILDDIR = $(AMBULANT_DIR)/build-3264
AMBULANT_3PP = $(AMBULANT_BUILDDIR)/third_party_packages
AMBULANT_LIB_3PP = $(AMBULANT_3PP)/installed/lib
AMBULANT_INCLUDE_3PP = $(AMBULANT_3PP)/installed/include
ARCHS = x86_64 i386
LIBRARY_SEARCH_PATHS = $(inherited) $(AMBULANT_LIB_3PP) $(AMBULANT_3PP)/live/BasicUsageEnvironment $(AMBULANT_3PP)live/groupsock $(AMBULANT_3PP)/liveMedia $(AMBULANT_3PP)/live/UsageEnvironment
USER_HEADER_SEARCH_PATHS = $(SRCROOT)/../../include $(AMBULANT_INCLUDE_3PP) $(AMBULANT_INCLUDE_3PP)/SDL $(AMBULANT_3PP)/live/BasicUsageEnvironment/include $(AMBULANT_3PP)/live/groupsock/include $(AMBULANT_3PP)/live/liveMedia/include $(AMBULANT_3PP)/live/UsageEnvironment/include